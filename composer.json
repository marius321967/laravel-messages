    "description": "Store text messages and dump them in user's view",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
			"marius321967\\Messages\\": "src/"
		}
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0"
    },

    "extra": {
        "laravel": {
            "providers": [
                "marius321967\\Messages\\MessageServiceProvider"
            ]
        }
    }
}
